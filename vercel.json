{
  "version": 2,
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/",
      "destination": "/api/index"
    },
    {
      "source": "/kontak_saya",
      "destination": "/api/kontak_saya"
    },
    {
      "source": "/coworker",
      "destination": "/api/coworker"
    },
    {
      "source": "/devteam",
      "destination": "/api/devteam"
    },
    {
      "source": "/(.*)",
      "destination": "/api/[file]?file=$1"
    }
  ]
}